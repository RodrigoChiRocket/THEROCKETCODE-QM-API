<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qualitas.portal.fraudes.account.dao.MovimientoReservaDao">

    <!-- Mapper para obtener MovimientoReserva por ID -->
    <select id="obtenerMovimientoReserva" resultMap="MovimientoReservaResult">
        SELECT *FROM T_MOVIMIENTO_RESERVA m
        WHERE I_MOV_RESERVA_ID = #{id}
    </select>

    <!-- ResultMap para mapear el resultado a la clase MovimientoReserva -->
    <resultMap id="MovimientoReservaResult" type="com.qualitas.portal.fraudes.account.model.MovimientoReserva">
        <id property="iMovReserva" column="I_MOV_RESERVA_ID"/>
        <result property="vTipoMovimiento" column="V_TIPO_MOVIMIENTO"/>
        <result property="iCoberturaID" column="I_COBERTURA_ID"/>
        <result property="dImporte" column="D_IMPORTE"/>
        <result property="iSiniestroID" column="I_SINIESTRO_ID"/>
        <result property="dFechaMovimiento" column="D_FECHA_MOVIMIENTO"/>
    </resultMap>



</mapper>
